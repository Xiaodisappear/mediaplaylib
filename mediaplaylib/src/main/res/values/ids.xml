<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 视频播放器Controller内通用ids -->

    <!-- 播放器标题 -->
    <item name="title" type="id"/>
    <!-- 返回上一页或者非全屏时退出播放 -->
    <item name="back" type="id"/>
    <!-- 返回上一页或者非全屏时退出播放 -->
    <item name="player_cover" type="id"/>

    <!-- 暂停/继续 -->
    <item name="pause_resume" type="id"/>
    <!-- 上一个 -->
    <item name="prev" type="id"/>
    <!-- 下一个 -->
    <item name="next" type="id"/>
    <!-- 快退 -->
    <item name="rew" type="id"/>
    <!-- 快进 -->
    <item name="ff" type="id"/>

    <!-- 播放当前时长 -->
    <item name="time_current" type="id"/>
    <!-- 播放总时长 -->
    <item name="time_total" type="id"/>
    <!-- 播放进度条 -->
    <item name="play_progressbar" type="id"/>
    <!-- 播放器全屏 -->
    <item name="fullscreen" type="id"/>

    <!-- 播放器顶部控制条 -->
    <item name="controller_top" type="id"/>
    <!-- 播放器底部控制条 -->
    <item name="controller_bottom" type="id"/>
    <!-- 播放器左边控制显示区域 -->
    <item name="controller_left" type="id"/>
    <!-- 播放器右边控制显示区域 -->
    <item name="controller_right" type="id"/>
    <!-- 播放器中部控制区 -->
    <item name="controller_center" type="id"/>

</resources>